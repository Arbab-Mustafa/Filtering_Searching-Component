{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/events/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/venues/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/guests/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/emails/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
